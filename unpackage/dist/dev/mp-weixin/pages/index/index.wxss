.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
swiper {
  width: 750rpx;
  height: 300rpx;
}
swiper swiper-item {
  width: 100%;
  height: 100%;
}
swiper swiper-item image {
  width: 100%;
  height: 100%;
}
.nav {
  width: 750rpx;
  height: 150rpx;
  display: flex;
  justify-content: space-around;
  padding: 10rpx 0;
}
.nav .nav-item {
  width: 150rpx;
  height: 150rpx;
}
.nav .nav-item image {
  width: 100%;
  height: 100%;
}
.floor {
  width: 98%;
  height: 450rpx;
  margin: 50rpx 15rpx;
}
.floor .title {
  width: 100%;
  height: 100rpx;
}
.floor .title image {
  width: 100%;
  height: 100%;
}
.floor .floor-left {
  float: left;
  width: 230rpx;
  height: 350rpx;
}
.floor .floor-left image {
  width: 100%;
  height: 100%;
}
.floor .floor-right {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  align-content: space-between;
  float: left;
  margin-left: 10rpx;
  width: 480rpx;
  height: 350rpx;
}
.floor .floor-right .floor-right-item {
  width: 49%;
  height: 49%;
}
.floor .floor-right .floor-right-item image {
  width: 100%;
  height: 100%;
}

